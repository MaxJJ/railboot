<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="topCenterHBox" fillHeight="false" maxHeight="-Infinity" prefHeight="130.0" prefWidth="1200.0" spacing="6.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lv.rtme.fxui.mainView.TopPaneController">
   <children>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="130.0" prefWidth="200.0" style="-fx-background-color: #F0FAFF;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="50.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Всего файлов: " GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <Label fx:id="tottalLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Без заказов: " GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
            <Label fx:id="notOrderedLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Не запрошено ставок:  " GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
            <Label fx:id="notRequestedLabel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Text fx:id="dateText" fill="#081943" stroke="#8b8c94" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" GridPane.halignment="RIGHT">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </GridPane>
      <StackPane fx:id="centerStackPane" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="130.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="infoText" strokeType="OUTSIDE" strokeWidth="0.0" text="ВЫБЕРИ  ФАЙЛ ИЗ ТАБЛИЦЫ" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
         </children>
      </StackPane>
   </children>
</HBox>
