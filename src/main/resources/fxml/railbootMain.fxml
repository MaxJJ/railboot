<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lv.rtme.fxui.mainView.RailbootMainController">
   <children>
      <AnchorPane maxWidth="-Infinity" style="-fx-background-color: #F7FCFF;">
         <children>
            <VBox spacing="8.0" style="-fx-background-color: #F7FCFF;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox fx:id="topCenterHBox" prefHeight="130.0" prefWidth="200.0" VBox.vgrow="NEVER" />
                  <HBox fx:id="toolHBox" prefHeight="35.0" spacing="4.0">
                     <children>
                        <ToolBar prefHeight="40.0" prefWidth="350.0">
                          <items>
                            <Button maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Новый">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Копировать">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                              </Button>
                              <Button layoutX="68.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Удалить">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                              </Button>
                              <Button layoutX="154.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Отменить">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                              </Button>
                          </items>
                        </ToolBar>
                        <ToolBar prefHeight="40.0" prefWidth="200.0">
                          <items>
                            <Button mnemonicParsing="false" text="Button" />
                          </items>
                        </ToolBar>
                        <ToolBar prefHeight="40.0" prefWidth="200.0">
                          <items>
                            <Button mnemonicParsing="false" text="Button" />
                          </items>
                        </ToolBar>
                     </children></HBox>
                  <TabPane fx:id="mainTabPane" style="-fx-background-color: #F7FCFF;" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="8.0" />
                     </VBox.margin>
                     <tabs>
                        <Tab fx:id="homeTab" text="Home">
                          <content>
                              <StackPane fx:id="homeTabStackPane" prefWidth="1200.0" />
                          </content>
                        </Tab>
                        <Tab text="Таблица">
                           <content>
                              <ScrollPane>
                                 <content>
                                    <VBox spacing="6.0">
                                       <children>
                                          <HBox spacing="4.0">
                                             <children>
                                                <TextField fx:id="searchTextField" prefWidth="350.0" promptText="ПОИСК" />
                                              <Button fx:id="notOrderedButton" mnemonicParsing="false" prefWidth="100.0" text="Не заказаные" />
                                                <Button fx:id="showAllButton" mnemonicParsing="false" onAction="#fillAllItems" prefWidth="100.0" text="Все" />
                                                <Pane nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Button fx:id="newButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Создать новый" textAlignment="CENTER" />
                                                   </children>
                                                   <HBox.margin>
                                                      <Insets right="4.0" />
                                                   </HBox.margin>
                                                </Pane>
                                             </children>
                                             <VBox.margin>
                                                <Insets left="3.0" top="4.0" />
                                             </VBox.margin>
                                          </HBox>
                                          <AnchorPane fx:id="tableAnchorPane">
                                             <children>
                                                <TableView fx:id="codesOrdersTable" nodeOrientation="LEFT_TO_RIGHT" prefWidth="1200.0" tableMenuButtonVisible="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                  <columns>
                                                    <TableColumn fx:id="fileID" maxWidth="-1.0" minWidth="80.0" prefWidth="-1.0" text="ФАЙЛ" />
                                                    <TableColumn fx:id="stDispatch" prefWidth="297.0" text="СТАНЦИЯ ОТПРАВЛЕНИЯ" />
                                                      <TableColumn fx:id="stDestination" prefWidth="321.0" text="СТАНЦИЯ  НАЗНАЧЕНИЯ" />
                                                      <TableColumn prefWidth="75.0" text="ОПИСАНИЕ" />
                                                      <TableColumn fx:id="cargo" prefWidth="75.0" text="ГРУЗ" />
                                                      <TableColumn fx:id="wagon" prefWidth="75.0" text="ВАГОН" visible="false" />
                                                      <TableColumn fx:id="container" prefWidth="75.0" text="КОНТЕЙНЕР" visible="false" />
                                                      <TableColumn fx:id="rate" prefWidth="75.0" text="СТАВКА" />
                                                  </columns>
                                                   <columnResizePolicy>
                                                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                   </columnResizePolicy>
                                                </TableView>
                                             </children>
                                          </AnchorPane>
                                       </children>
                                    </VBox>
                                 </content>
                              </ScrollPane>
                           </content>
                        </Tab>
                        <Tab text="Станции">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                          </content>
                        </Tab>
                     </tabs>
                  </TabPane>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="3.0" left="10.0" right="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
